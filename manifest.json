{
    "manifest_version": 2,
    "name": "Language Translator",
    "description": "This extension will translate langugage and provide pronunciation",
    "version": "1.2",
    "browser_action": {
        "default_icon": {
            "32": "translate.png"
        },
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}